{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "zetkin/join-form-block",
	"version": "0.1.0",
	"title": "Zetkin Join Form",
	"category": "widgets",
	"icon": "admin-users",
	"description": "Embeds a Zetkin join form.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"formId": {
			"type": "number",
			"default": 0
		},
		"formSubmitToken": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "zetkin",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
